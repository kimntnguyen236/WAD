@model IEnumerable<Lab10.Models.vSkillProgrammer>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutVisitor.cshtml";
}

<h1>Programmer List</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProgrammerCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProgrammerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SkillName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salary)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProgrammerCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProgrammerName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SkillName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Salary)
                </td>
                @*<td>
                        <a asp-action="Edit" asp-route-id="@item.ProgrammerCode">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.ProgrammerCode">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.ProgrammerCode">Delete</a>
                    </td>*@
            </tr>
        }
    </tbody>
</table>
