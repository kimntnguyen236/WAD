@model Lab8.Models.Order
@{
    ViewData["Title"] = "Create";
}
<h1>Create new Order</h1>
<br />
@using (Html.BeginForm())
{
    <div>
        <div>
            @Html.LabelFor(model => model.OrderDate)
            @Html.EditorFor(model => model.OrderDate)
        </div>
        <div>
            @Html.LabelFor(model => model.ProductName)
            @Html.EditorFor(model => model.ProductName)
        </div>
        <div>
            @Html.LabelFor(model => model.Price)
            @Html.EditorFor(model => model.Price)
        </div>
        <div>
            @Html.LabelFor(model => model.Quantity)
            @Html.EditorFor(model => model.Quantity)
        </div>
        <div>
            @Html.LabelFor(model => model.CustomerCode, "CustomerName")
            @Html.DropDownListFor(model => model.CustomerCode, (IEnumerable<SelectListItem>)ViewBag.cusName)
        </div>
        <br />
        <div>
            <input type="submit" name="Submit" value="Create" class="btn-primary" />
        </div>
    </div>

}

